{
  "name": "updatelearning",
  "version": "1.0.0",
  "productName": "Update Test",
  "description": "A simple electron app to learn how to update that.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build:linux": "electron-builder -l",
    "build:win": "electron-builder -w",
    "build:lw": "electron-builder -lw"
  },
  "build": {
    "appId": "com.niksingh710.test",
    "linux": {
      "target": ["AppImage", "deb", "snap"],
      "category": "Network",
      "icon": "./build/icon.icns",
      "executableName": "Update Test"
    },
    "win": {
      "target": ["nsis"],
      "icon": "./build/icon.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "author": "Nikhil Singh <nik.singh710@gmail.com>",
  "license": "ISC",
  "homepage": "https://www.github.com/niksingh710",
  "devDependencies": {
    "electron": "^11.2.1",
    "electron-builder": "^22.9.1",
    "electron-reloader": "^1.2.0",
    "nodemon": "^2.0.7"
  }
}